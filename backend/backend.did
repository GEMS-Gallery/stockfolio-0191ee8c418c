type Holding = 
 record {
   acquisitionDate: int;
   acquisitionPrice: float64;
   currentPrice: float64;
   id: nat;
   name: text;
   performance: float64;
   quantity: nat;
   symbol: text;
 };
service : {
  addHolding: (text, nat, float64) -> (nat);
  deleteHolding: (nat) -> (bool);
  getAllHoldings: () -> (vec Holding) query;
  getAveragePerformance: () -> (float64) query;
  getTotalPortfolioValue: () -> (float64) query;
  updateHolding: (nat, nat) -> (bool);
}
